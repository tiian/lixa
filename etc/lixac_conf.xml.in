        <rsrmgr>OracleXE</rsrmgr>
        <rsrmgr>LIXAdummyRM</rsrmgr>
      </rsrmgrs>
    </profile>
    <profile name="AG71">
      <trnmgrs>
        <trnmgr>local_2</trnmgr>
      </trnmgrs>
      <rsrmgrs>
        <rsrmgr>IBMDB2_dynreg</rsrmgr>
      </rsrmgrs>
    </profile>
    <profile name="CF70">
      <trnmgrs>
        <trnmgr>local_1</trnmgr>
      </trnmgrs>
    </profile>
  </profiles>
